{
  "hosting": {
    "site": "eternalquill-pm-dev",
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "functions": {
    "source": "firebase-functions",
    "runtime": "nodejs18",
    "params": "params.json",
     "defaultRegion": "asia-south1"
  }
}
